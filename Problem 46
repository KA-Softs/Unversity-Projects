//Input 2 numbers and print YES if one number is divisible by other

#include<iostream>
using namespace std;

int main()
{
    int n1,n2;

    cout<<"Enter first number : ";
    cin>>n1;
    cout<<"\nEnter second number : ";
    cin>>n2;

    if(n1%n2==0)
      cout<<"\nYES";
    else if(n2%n1==0)
     cout<<"\nYES";
    else 
     cout<<"\nNO";

    return 0;    
}
